<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.telegramdaily.medical</string>
    <key>ProgramArguments</key>
    <array>
        <string>/opt/anaconda3/envs/finance-env/bin/python</string>
        <string>/Users/tomadshead/telegramdaily/run_weekly_medical.py</string>
        <string>--local-llm</string>
    </array>
    <key>WorkingDirectory</key>
    <string>/Users/tomadshead/telegramdaily</string>
    <key>StartCalendarInterval</key>
    <dict>
        <key>Weekday</key>
        <integer>1</integer>
        <key>Hour</key>
        <integer>10</integer>
        <key>Minute</key>
        <integer>0</integer>
    </dict>
    <key>StandardOutPath</key>
    <string>/Users/tomadshead/telegramdaily/logs/medical_weekly.log</string>
    <key>StandardErrorPath</key>
    <string>/Users/tomadshead/telegramdaily/logs/medical_weekly.err</string>
</dict>
</plist>
