<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.telegramdaily.macro</string>
    <key>ProgramArguments</key>
    <array>
        <string>/opt/anaconda3/envs/finance-env/bin/python</string>
        <string>-u</string>
        <string>/Users/tomadshead/telegramdaily/run_daily_macro.py</string>
        <string>--local-llm</string>
    </array>
    <key>WorkingDirectory</key>
    <string>/Users/tomadshead/telegramdaily</string>
